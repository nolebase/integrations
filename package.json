{
  "name": "@nolebase/integrations",
  "type": "module",
  "version": "1.15.0",
  "description": "A collection of diverse documentation engineering tools.",
  "author": {
    "name": "Nólëbase",
    "email": "nolebase@ayaka.moe",
    "url": "https://github.com/nolebase"
  },
  "license": "MIT",
  "keywords": [
    "vitepress",
    "nolebase",
    "markdown",
    "vitepress-plugin",
    "nolebase-integration"
  ],
  "scripts": {
    "stub": "pnpm -r --filter=./packages/* --parallel run stub",
    "lint": "eslint --cache .",
    "docs:dev": "pnpm -r --filter=./docs run dev",
    "docs:build": "pnpm -r --filter=./docs run build",
    "docs:preview": "pnpm -r --filter=./docs run preview",
    "packages:dev": "pnpm cleanup:dist && pnpm -r --filter=./packages/* run dev",
    "packages:stub": "pnpm -r --filter=./packages/* run stub",
    "packages:build": "pnpm cleanup:dist && pnpm -r --filter=./packages/* run build",
    "cleanup:dist": "rimraf --glob **/dist",
    "cleanup:node_modules": "rimraf --glob **/node_modules",
    "publish": "pnpm packages:build && pnpm -r --filter='!@nolebase/integrations-docs' exec pnpm publish --access=public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@antfu/ni": "^0.21.12",
    "@iconify-json/carbon": "^1.1.27",
    "@iconify-json/icon-park-outline": "^1.1.15",
    "@iconify-json/octicon": "^1.1.52",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@types/node": "^20.10.6",
    "@unocss/eslint-config": "^0.58.2",
    "@unocss/eslint-plugin": "^0.58.2",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vueuse/core": "^10.7.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "less": "^4.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.57.7",
    "unplugin-vue-macros": "^2.7.7",
    "vite": "^5.0.11",
    "vitepress": "1.0.0-rc.38",
    "vue": "^3.4.14",
    "vue-tsc": "^1.8.27"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ]
}
