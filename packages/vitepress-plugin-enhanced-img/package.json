{
  "name": "@nolebase/vitepress-plugin-enhanced-img",
  "type": "module",
  "version": "1.27.2",
  "description": "A VitePress plugin that empowers the images referenced in raw, as well as the <img> tags in markdown files.",
  "author": {
    "name": "Nólëbase",
    "email": "nolebase@ayaka.moe",
    "url": "https://github.com/nolebase"
  },
  "license": "MIT",
  "homepage": "https://nolebase-integrations.ayaka.io/pages/en/integrations/vitepress-plugin-enhanced-img/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nolebase/integrations.git",
    "directory": "packages/vitepress-plugin-enhanced-img"
  },
  "keywords": [
    "vitepress",
    "nolebase",
    "markdown",
    "thumbhash",
    "blurhash",
    "unlazy",
    "vite",
    "vitepress-plugin",
    "nolebase-integration"
  ],
  "sideEffects": false,
  "exports": {
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    },
    "./markdown-it": {
      "types": "./dist/markdown-it/index.d.ts",
      "import": "./dist/markdown-it/index.mjs",
      "require": "./dist/markdown-it/index.cjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.mjs",
      "require": "./dist/client/index.js"
    }
  },
  "main": "./dist/vite/index.cjs",
  "module": "./dist/vite/index.mjs",
  "types": "./dist/vite/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "package:publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.51",
    "colorette": "^2.0.20",
    "glob": "^10.3.10",
    "markdown-it": "^13.0.2",
    "ora": "^8.0.1",
    "thumbhash": "^0.1.1",
    "uncrypto": "^0.1.3",
    "unlazy": "^0.11.2",
    "vitepress": "^1.0.0-rc.45"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.7",
    "builtin-modules": "^3.3.0"
  }
}
