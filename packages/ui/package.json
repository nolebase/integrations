{
  "name": "@nolebase/ui",
  "type": "module",
  "version": "2.0.0-rc4",
  "description": "A collection of Vue components Nolebase uses.",
  "author": {
    "name": "Nólëbase",
    "email": "nolebase@ayaka.moe",
    "url": "https://github.com/nolebase"
  },
  "license": "MIT",
  "homepage": "https://nolebase-integrations.ayaka.io/pages/en/ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nolebase/integrations.git",
    "directory": "packages/ui"
  },
  "keywords": [
    "unjs",
    "vue",
    "vite",
    "vitepress",
    "vitepress-plugin",
    "components",
    "component-library",
    "markdown",
    "frontmatter",
    "nolebase",
    "nolebase-integration"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "package:publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "vitepress": "^1.0.0",
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "vitepress": {
      "optional": true
    }
  },
  "dependencies": {
    "@rive-app/canvas": "^2.11.1",
    "asciinema-player": "^3.7.1",
    "less": "^4.2.0"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.5.1"
  }
}
