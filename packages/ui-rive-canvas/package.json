{
  "name": "@nolebase/ui-rive-canvas",
  "type": "module",
  "version": "2.10.0",
  "description": "Wrapper of `@rive-app/canvas` for VitePress documentation sites.",
  "author": {
    "name": "Nólëbase",
    "email": "nolebase@ayaka.moe",
    "url": "https://github.com/nolebase"
  },
  "license": "MIT",
  "homepage": "https://nolebase-integrations.ayaka.io/pages/en/ui/lazy-teleport-rive-canvas/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nolebase/integrations.git",
    "directory": "packages/ui-rive-canvas"
  },
  "keywords": [
    "unjs",
    "vue",
    "vite",
    "vitepress",
    "vitepress-plugin",
    "components",
    "component-library",
    "markdown",
    "frontmatter",
    "nolebase",
    "nolebase-integration",
    "lottie",
    "rive"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/style.css",
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "package:publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "@rive-app/canvas": "^2.11.1"
  },
  "dependencies": {
    "less": "^4.2.0",
    "vitepress": "^1.5.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.6.0"
  }
}
